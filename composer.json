{
    "name": "san4io/request-logger",
    "description": "Laravel Request Response Logger",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "illuminate/support": "5.*",
        "illuminate/routing": "5.*",
        "illuminate/http": "5.*",
        "illuminate/queue": "5.*",
        "monolog/monolog": "~1.12",
        "symfony/http-foundation": "~3.3"
    },
    "author": {
        "name": "Aleksandr Golubovskij",
        "email": "alegol90@gmail.com"
    },
    "autoload": {
        "psr-4": {
            "San4io\\RequestLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "San4io\\RequestLogger\\RequestLoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}